function elmBootstrap () {
    var app = Elm.fullscreen(Elm.Main);
    app.ports.modalRequests.subscribe(function (modalId) {
        $(modalId).openModal({
            dismissible: true,
            opacity: 0.5, 
            in_duration: 300, 
            out_duration: 200,
        });
    });
}

elmBootstrap();
